{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "jquery"
  ],
  "loadEagerly": [
    "src/angular/**/*.js"
  ],
  "dontLoad": [
    "node_module/**"
  ],
  "plugins": {
    "doc_comment": true,
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "angular": {},
    "complete_strings": {
      "maxLength": 15
    }
  }
}
